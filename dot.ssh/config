ServerAliveInterval 60

TCPKeepAlive yes
ServerAliveInterval 15
ServerAliveCountMax 16

# This will add your keys to the macOS keychain, so it remembers the key's passphrase
AddKeysToAgent yes
UseKeychain yes

host github.com
  user jmyounker

ControlPath ~/.ssh/control-%r@%h:%p
ControlMaster auto
ControlPersist 1

