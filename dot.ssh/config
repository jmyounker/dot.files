ServerAliveInterval 60

host github.com
  user jmyounker

# SavingGlobal hosts

host ci jenkins
  HostName ci.savingglobal.com
  User jyounker
  IdentityFile ~/.ssh/id_rsa

host ssh2
  HostName ssh2.savingglobal.com
  User jyounker
  LocalForward 3128 localhost:3128
  ForwardAgent yes
  LogLevel quiet

Host gitlab.savingglobal.com
  IdentityFile ~/.ssh/id_rsa

Host 136.243.209.* 148.251.203.*
  Hostname %h
  User svgl
  StrictHostKeyChecking no
  ForwardAgent yes
  ExitOnForwardFailure yes
  ProxyCommand ssh ssh2 -W %h:%p

Host remote-testsparen-*
  Hostname %h.savingglobal.com
  User svgl
  StrictHostKeyChecking no
  ForwardAgent yes
  ExitOnForwardFailure yes
  ProxyCommand ssh ssh2 -W %h:%p

host upsource
 Hostname 148.251.18.202
 User root
 ForwardAgent no
 IdentityFile ~/.ssh/id_rsa

