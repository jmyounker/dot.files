# SavingGlobal hosts

host vagrant
 Hostname 127.0.0.1
 Port 2222
 User vagrant
 ForwardAgent yes
 ForwardX11 yes
 IdentityFile ~/dev/svn/opsys/trunk/opsys/vagrant/init/.vagrant/machines/default/virtualbox/private_key

host yum
 Hostname 148.251.18.203
 User yumman

host ci jenkins
  HostName ci.savingglobal.com
  User jyounker
  IdentityFile ~/.ssh/id_rsa

host ssh2
  HostName ssh2.savingglobal.com
  User jyounker
  LocalForward 3128 localhost:3128
  ForwardAgent yes
  LogLevel quiet

Host gitlab.savingglobal.com
  IdentityFile ~/.ssh/id_rsa

host upsource
