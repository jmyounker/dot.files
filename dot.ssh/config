 Hostname 127.0.0.1
 Port 2222
 User vagrant
 ForwardAgent yes
 ForwardX11 yes
 IdentityFile ~/dev/svn/opsys/work/vagrant/init/.vagrant/machines/default/virtualbox/private_key

host yum
 Hostname 148.251.18.203
 User yumman

