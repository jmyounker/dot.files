# Harpoon development at SoundCloud
ServerAliveInterval 300

host harp-vm01 harp harpoon-vm01 harpoon
  hostname 192.168.169.4
  user vagrant
  ForwardAgent yes
  ForwardX11 yes
  IdentityFile /Users/jeff/.vagrant.d/insecure_private_key

GEN:./scripts/gen_soundcloud_ssh_hosts dot.ssh/soundcloud_hosts

# SavingGlobal hosts

host jenkins
  HostName ci.savingglobal.com
  User jyounker
  IdentityFile ~/.ssh/id_rsa

host ssh2
  HostName ssh2.savingglobal.com
  User jyounker
  LocalForward 3128 localhost:3128
  ForwardAgent yes
  LogLevel quiet

Host gitlab.savingglobal.com
  IdentityFile ~/.ssh/id_rsa

Host 136.243.209.* 148.251.203.*
  Hostname %h
  User svgl
  StrictHostKeyChecking no
  ForwardAgent yes
  ExitOnForwardFailure yes
  ProxyCommand ssh ssh2 -W %h:%p

Host remote-testsparen-*
  Hostname %h.savingglobal.com
  User svgl
  StrictHostKeyChecking no
  ForwardAgent yes
  ExitOnForwardFailure yes
  ProxyCommand ssh ssh2 -W %h:%p
