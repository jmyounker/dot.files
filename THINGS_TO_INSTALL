When I install onto a new machine I should install:

sudo pip3 install virtualenvwrapper

# Zgen
git clone https://github.com/tarjoilija/zgen.git "${HOME}/.zgen"

# Brew
/bin/bash -c "$(curl -fsSL https://raw.githubusercontent.com/Homebrew/install/HEAD/install.sh)"
bew install cask
brew install tig
brew install tmux
brew install reattach-to-user-namespace
brew install --cask iterm2
brew install --cask zoom

compaudit | xargs chmod g-w,o-w


brew install --cask slack

# Import iterm config from artifacts/iterm-profile.json

# Install powerline (zgen) fonts
unzip artifacts/fonts-master.zip
cd fonts-master
./install.sh
 
# Import hazel rules from artifacts/hazel

